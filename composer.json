{
  "name": "macavity/phpbb-sessions-auth-bundle",
  "description": "A symfony bundle that provides symfony the ability to integrate and share sessions with phpBB and use phpBB as the authentication provider.",
  "keywords": [
    "phpBB",
    "intergration",
    "bridge"
  ],
  "version": "1.2.1",
  "type": "symfony-bundle",
  "homepage": "http://www.phpbb.com",
  "license": "MIT",
  "authors": [
    {
      "name": "phpBB Website team",
      "email": "website@phpbb.com",
      "homepage": "https://www.phpbb.com/"
    }
  ],
  "support": {
    "irc": "irc://irc.freenode.org/phpbb-dev",
    "issues": "https://www.phpbb.com/bugs/website/"
  },
  "require": {
    "symfony/framework-bundle": "~2.3|^3.0|^4.0",
    "symfony/twig-bundle": "~2.3|^3.0|^4.0",
    "symfony/form": "~2.3|^3.0|^4.0",
    "symfony/security-bundle": "~2.3|^3.0|^4.0",
    "doctrine/common": "~2.1",
    "doctrine/orm": "~2.1"
  },
  "require-dev": {
    "phpunit/phpunit": "*",
    "symfony/var-dumper": "*"
  },
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {"phpBB\\SessionsAuthBundle\\": ""}
  }
}
